  agent any;

  stages {
    stage('build') {
      steps {
        echo 'building...'
      }
    }
    stage('test') {
      steps {
        echo 'testing...'
      }
    }
    stage('deploy') {
      steps {
        echo 'deploying...'
      }
    }
  }
  stage('hello world') {
      steps {
        echo 'hello world!'
      }
    }
}
